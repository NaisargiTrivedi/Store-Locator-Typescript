<div class="col-md-4 col-2">
    <label for="storeName" class="form-label">Store name</label>
    <% if(typeof(errMsg) !== 'undefined'){ %>
      <input required type="text" class="form-control" id="storeName" name="storeName" value= <%= req.body.storeName %>>
    <div class="text-danger">
      <%= errMsg.storeVal %>
    </div>
    <% } else{ %>
      <input required type="text" class="form-control" id="storeName" name="storeName">
      <div class="text-danger" id="storeVal"></div>
    <% } %>
  </div>
  <div class="col-md-4 col-2">
    <label for="city" class="form-label">City</label>
    <% if(typeof(errMsg) !== 'undefined'){ %>
      <input type="text" class="form-control" id="city" name="city" required value= <%= req.body.city %>>
      <div class="text-danger">
        <%= errMsg.cityVal %>
      </div>
      <% } else{  %>
    <input type="text" class="form-control" id="city" name="city" required>
    <div class="text-danger" id="cityVal"></div>
    <% } %>
  </div>
  <div class="col-md-4 col-2">
    <label for="state" class="form-label">State</label>
    <% if(typeof(errMsg) !== 'undefined'){ %>
      <input type="text" class="form-control" id="state" name="state" required value= <%= req.body.state %>>
      <div class="text-danger">
        <%= errMsg.stateVal %>
      </div>
      <% } else{  %>
      <input type="text" class="form-control" id="state" name="state" required>
      <div class="text-danger" id="stateVal"></div>
    <% } %>
  </div>
  <div class="col-md-6 col-3">
    <label for="address" class="form-label">Address</label>
    <% if(typeof(errMsg) !== 'undefined'){ %>
      <input type="text" class="form-control" id="address"  name="address" required value= <%= req.body.address %>>
      <div class="text-danger">
        <%= errMsg.addVal %>
      </div>
      <% } else{  %>
      <input type="text" class="form-control" id="address"  name="address" required>
      <div class="text-danger" id="addVal"></div>
    <% } %>
  </div>
  <div class="col-md-3 col-2">
    <label for="postalCode" class="form-label">Postal Code</label>
    <% if(typeof(errMsg) !== 'undefined'){ %>
      <input type="text" class="form-control" id="postalCode" name="postalCode" required value= <%= req.body.postalCode%>>
      <div class="text-danger">
        <%= errMsg.postalVal %>
      </div>
      <% } else{  %>
    <input type="text" class="form-control" id="postalCode" name="postalCode" required>
    <div class="text-danger" id="postalVal"></div>
      <% } %>
  </div>
  <div class="col-md-3 col-2">
    <label for="country" class="form-label">Country</label>
    <% if(typeof(errMsg) !== 'undefined'){ %>
      <input type="text" class="form-control" id="country" name="country" required value=<%= req.body.country %>>
      <div class="text-danger">
        <%= errMsg.countryVal %>
      </div>
      <% } else{  %>
    <input type="text" class="form-control" id="country" name="country" required>
    <div class="text-danger" id="countryVal"></div>
    <% } %>
  </div>
  <div class="col-6">
      <label for="logo" class="form-label">Logo</label>
      <input type="file" class="form-control" id="logo" name="logo">
      <div class="invalid-feedback">
          Please provide a logo.
      </div>
    </div>
  <div class="col-12">
    <div class="form-check">
      <input class="form-check-input" type="checkbox" id="status" name="status">
      <label class="form-check-label" for="status">
        Status
      </label>
      <div class="invalid-feedback">
        You must provide you status.
      </div>
    </div>
  </div>  